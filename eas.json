{
  "cli": {
    "version": ">= 16.25.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api-whisp.onrender.com",
        "EXPO_PUBLIC_SOCKET_URL": "https://api-whisp.onrender.com",
        "EXPO_PUBLIC_DEV_MODE": "false"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api-whisp.onrender.com",
        "EXPO_PUBLIC_SOCKET_URL": "https://api-whisp.onrender.com",
        "EXPO_PUBLIC_DEV_MODE": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
